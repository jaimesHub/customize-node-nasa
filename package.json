{
	"name": "custom-n8n-nodes-nasapics-kdq",
	"version": "1.0.0",
	"description": "n8n node to call NASA's APOD and Mars Rover Photo services by KhanhDQ12",
	"keywords": [
		"n8n-community-node-package"
	],
	"license": "MIT",
	"homepage": "https://docs.n8n.io/integrations/creating-nodes/build/",
	"author": {
		"name": "khanhdq",
		"email": "duongquockhanh230596@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/jaimesHub/customize-node-nasa"
	},
	"engines": {
		"node": ">=20.18.0"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/NasaPicsApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/NasaPics/NasaPics.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "^7.15.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.1",
		"gulp": "^4.0.2",
		"n8n-workflow": "*",
		"prettier": "^3.3.2",
		"typescript": "^5.5.3",
		"eslint": "^8.56.0"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
